          -->
        <security allowRemoteAccess="false"/>
        <errorLog type="Elmah.XmlFileErrorLog, Elmah" logPath="~/App_Data/ElmahErrors"/>
        <errorFilter>
            <test>
                <equal binding="HttpStatusCode" value="404" type="Int32"/>
            </test>
        </errorFilter>
    </elmah>
